{
  "name": "@finn.auto/sls-openapi-doc-generator",
  "version": "2.1.2",
  "description": "Serverless 1.0 plugin to generate OpenAPI V3 documentation from serverless configuration",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.14.6"
  },
  "repository": "https://github.com/jakubzzak/serverless-openapi-documentation.git",
  "bugs": "https://github.com/jakubzzak/serverless-openapi-documentation/issues",
  "author": "Abilio Henrique <abilio.henrique@temando.com>",
  "contributors": [
    "Abilio Henrique <abilio.henrique@temando.com>",
    "nfour <novus.nfour@gmail.com>",
    "jakubzzak <jakub.zak@finn.auto>"
  ],
  "keywords": [
    "OpenAPI",
    "swagger",
    "serverless"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest -c ./jest.config.js",
    "test:build": "jest -c '{ \"testRegex\": \".spec.js$\"}' build",
    "test:coverage": "jest -c ./jest.config.js --coverage",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "yarn run lint -- --fix",
    "preversion": "yarn run lint && yarn run build && yarn run test:build && changelog-verify CHANGELOG.md",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md",
    "release": "cd build && yarn publish",
    "build:link": "yarn run build && cd build && yarn link",
    "build:watch": "yarn run build && tsc --watch",
    "build": "scripts/build.bash",
    "test:prepare": "scripts/prepareTests.bash",
    "test:gen": "cd test/css && ./node_modules/.bin/sls openapi generate",
    "test:genjson": "cd test/css && ./node_modules/.bin/sls openapi generate -f json",
    "test:doc": "yarn test:prepare && yarn test:gen",
    "test:docjson": "yarn test:prepare && yarn test:genjson",
    "test:offline": "cd test/css && sls offline && cd ../.."
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@hapi/eslint-plugin": "^5.1.0",
    "@types/bluebird": "^3.5.8",
    "@types/chalk": "^0.4.31",
    "@types/fs-extra": "^4.0.0",
    "@types/jest": "^20.0.2",
    "@types/js-yaml": "^3.12.1",
    "@types/json-schema": "^7.0.3",
    "@types/lodash": "^4.14.123",
    "@types/node": "^8.10.48",
    "@types/serverless": "^1.18.2",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "changelog-verify": "^1.0.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.8.0",
    "openapi-types": "^1.3.4",
    "prettier": "^1.17.0",
    "serverless": "^1.41.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^3.1.0",
    "typescript": "^3.4.5",
    "version-changelog": "^3.1.1"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^2.0.1",
    "fs-extra": "^4.0.1",
    "js-yaml": "^3.8.4",
    "json-schema-ref-parser": "^6.1.0",
    "lodash": "^4.17.11",
    "swagger2openapi": "^2.5.0",
    "uuid": "^3.1.0"
  },
  "private": false
}
